import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int row = 1, digit = 1, k = 1;
        while(row<=n){
            int i = 1;
            while(i<=digit){
                System.out.print(k+"\t");
                i++;
                k++;
            }
            row++;
            digit++;
            System.out.println();
        }
        sc.close();
    }
}
