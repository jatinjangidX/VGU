import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int row = 1;
        int digit = 1;

        while(row<=n){
            int i =1;
            int val=1;
            while(i<=digit){
                    System.out.print(val+"\t");
                val = val*(row-i)/i;
                i++;
        }
        row++;
        digit++;
        System.out.println();
        }

    }
}
